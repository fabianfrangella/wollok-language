describe "operations"{
	
	test "negation" {
		
		assert.that(!false)
	
	}
	
	test "double negation" {
	
		assert.notThat(not ! false)
	
	}
	
	test "sum three members" {
	
		assert.equals(-0.5, -1.5 + -2 + 3)
	
	}

	test "arithmetic operation precedence" {
	
		assert.equals(3.8, 2 * 3 / 2 + 4 / 5)
		assert.equals(0.2, 2 * 3 / (2 + 4) / 5)
	}
	
}




