class BaseClass {
	method m() = 1
	method inherited() = "inherited"
}

class ChildClass inherits BaseClass {
	override method m() = 2
}

class MyClass {
}

describe "inheritance from object class" {

	test "Simple method inherited from object" {
		const myObject = new MyClass()
		assert.that(myObject.identity() != null)
	}

}

describe "basic inheritance" {

	test "override method" {
		assert.that(new ChildClass().m() == 2)
	}

	test "inherited method" {
		assert.equals("inherited", new ChildClass().inherited())
	}

}