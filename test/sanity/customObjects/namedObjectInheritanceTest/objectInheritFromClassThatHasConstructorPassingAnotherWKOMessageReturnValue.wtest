class Dog {
	const owner
	constructor(param) {
		owner = param
	}	
	method owner() = owner
}
			
object lassie inherits Dog(jorge.name()) {
}
			
object jorge {
	method name() = "Jorge"
}
			
test "object inherit from class that has constructor passing another wko message return value" {

	assert.equals("Jorge", lassie.owner())

}