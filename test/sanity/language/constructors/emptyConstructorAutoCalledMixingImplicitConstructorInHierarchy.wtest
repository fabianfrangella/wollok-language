class A {
	var x
	constructor() { x = 20 }

	method getX() { return x }
}

class B inherits A {
}

class C inherits B {
	var c1
	constructor() {
		c1 = 10
	}
	method getC1() { return c1 }
}

class D inherits C {}

test "empty constructor auto called mixing implicit constructor in hierarchy" {
	const o = new D()
	assert.equals(20, o.getX())
	assert.equals(10, o.getC1())
}